{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": false,
    "no-empty": false,
    "no-unused-variable": false,
    "no-unused-expression": false,
    "interface-name": false,
    "member-access": false,
    "no-console": false,
    "prefer-const": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
